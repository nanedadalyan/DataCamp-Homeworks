   "source": [
    "def repeatedString(str:String,n:Int): Int = {\n",
    "  val a = str * ((n / str.length) + 1)\n",
    "  val b = a.substring(0,n)\n",
    "  return b.count(_ =='a')\n",
    "}"
   ]
